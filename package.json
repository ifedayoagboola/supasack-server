{
  "name": "supasack-server",
  "version": "1.0.0",
  "description": "Supasack - African & Caribbean Food Marketplace Backend",
  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "ts-node-dev -r tsconfig-paths/register -r ts-node-dev --respawn ./src/server.ts",
    "prettier": "prettier \"**/*.+(ts|js|json)\"",
    "format": "npm run prettier -- --write",
    "db:prepare": "prisma format && prisma generate",
    "db:migrate": " npm run db:prepare && prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "create-admin": "node scripts/create-super-admin-secure.js",
    "check-roles": "node scripts/check-roles.js",
    "start": "npm run build && node dist/src/server.js",
    "prebuild": "rm -rf dist/*",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s . -o ./dist",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifedayoagboola/supasack-server.git"
  },
  "keywords": [
    "marketplace",
    "african-food",
    "caribbean-food",
    "multi-vendor",
    "delivery"
  ],
  "author": "Supasack Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifedayoagboola/supasack-server/issues"
  },
  "homepage": "https://github.com/ifedayoagboola/supasack-server#readme",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@types/nodemailer": "^6.4.17",
    "app-root-path": "^3.1.0",
    "aws-sdk": "^2.1340.0",
    "axios": "0.23.0",
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "google-auth-library": "^8.5.2",
    "helmet": "^5.1.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.5",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "save": "^2.9.0",
    "slug": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@prisma/client": "^6.11.1",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.18.126",
    "@types/slug": "^5.0.3",
    "@types/winston": "^2.4.4",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "prisma": "^6.11.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
